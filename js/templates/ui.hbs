<script type="text/x-handlebars" class="ui-audience-list">
	<table id="audience-list-table" class="table modal-table sortable">
		{{#if profiles}}
			<thead>
				<tr class="row item">
					<th></th>
					<th>Twitter User</th>
					<th>Status</th>
					<th style="text-align: center">Select</th>
					<th style="text-align: right">Actions</th>
				</tr>
			</thead>
			<tbody>
			{{#each profiles}}
				<tr class="row item">
					<td width="48" class="logo"><img src="{{profile_image_url_https}}"></td>
					<td>
						<h4>{{name}}</h4>
						<h5>{{screen_name}}</h5>
					</td>
					<td id="progress-{{profile_id}}" class="progress-profile">
						{{#showProgressBar progress}}
							<span class="progress-message">
								Fetching Tweets...
							</span>
							<div class="progress">
			  				<div class="progress-bar" role="progressbar" style="width: {{formatPercent progress}}%;">
			    				{{formatPercent progress}}%
			  				</div>
							</div>
						{{else}}
							{{#isQueued progress}}
								<strong>Waiting...</strong></br>
								Will fetch in background
							{{else}}
								Refreshed on {{updated_date}}</br>
								{{follower_count}} Followers Retrieved</br>
								{{tweet_count}} Tweets Classified
							{{/isQueued}}
						{{/showProgressBar}}
					<td style="vertical-align: middle; text-align: center;">
						<input type="checkbox" class="blend-checkbox" data-profile-id="{{profile_id}}">
					</td>
					<td align="right">
						<input type="button" class="btn btn-primary btn-view" data-view-profile-id="{{profile_id}}" value="View">&nbsp;
						{{#if query_hash}}
						<input type="button" id="btn-refresh-{{profile_id}}" class="btn btn-primary btn-refresh {{hiddenClassName progress}}" data-refresh-screen-name="{{query_hash}}" value="Refresh">&nbsp;
						{{else}}
						<input type="button" id="btn-refresh-{{profile_id}}" class="btn btn-primary btn-refresh {{hiddenClassName progress}}" data-refresh-screen-name="{{screen_name}}" value="Refresh">&nbsp;
						{{/if}}
						<input type="button" class="btn btn-remove btn-delete" data-view-profile-id="{{profile_id}}" value="x">
					</td>
				</tr>
			{{/each}}
			</tbody>
		{{else}}
		No profiles exist, yet. Go ahead and add some!
		{{/if}}
	</table>
	<script type="text/javascript">
	$('.blend-checkbox').on('change', function(e) {
		var checked = $('.blend-checkbox:checked');
		if (checked.length) {
			$('#audience-list-footer').show();
		}
		else {
			$('#audience-list-footer').hide();
		}
	});
	</script>
</script>

