<!-- Chart Controls -->
<script type="text/x-handlebars" class="ui-chart-datepicker">
	<div class="datepicker-placeholder form-group form-inline">
		<input type="text" name="start_date" class="form-control bs-datepicker" placeholder="Start Date" size="10" value="{{initial_start_date}}">
		<input type="text" name="end_date" class="form-control bs-datepicker" placeholder="End Date" size="10"><br />
		<input type="text" name="postal_code" class="form-control" placeholder="Postal / ZIP" size="12">
		<select class="form-control" name="distance">
			<option value="">Distance</option>
			<option value="1">1 Miles</option>
			<option value="2">2 Miles</option>
			<option value="3">3 Miles</option>
			<option value="4">4 Miles</option>
			<option value="5">5 Miles</option>
			<option value="10">10 Miles</option>
			<option value="25">25 Miles</option>
			<option value="50">50 Miles</option>
			<option value="75">75 Miles</option>
			<option value="100">100 Miles</option>
		</select>&nbsp;
		<input type="button" class="btn btn-primary" id="btn-applyfilters" value="Apply">
		<input type="button" class="btn btn-primary" id="btn-resetfilters" value="Reset">
		<script type="text/javascript">
			$('.bs-datepicker').datepicker();
			$('#btn-applyfilters').on('click', function(e) {
				var start_date = $('input[name="start_date"]').val() || null;
				var end_date = $('input[name="end_date"]').val() || null;
				var postal_code = $('input[name="postal_code"]').val() || null;
				var distance = $('select[name="distance"]').val() || null;
				EC.Events.publish('/ScatterPlot/applyFilters', {
					'start_date': start_date,
					'end_date': end_date,
					'postal_code': postal_code,
					'distance': distance
				});
			});
			$('#btn-resetfilters').on('click', function(e) {
				$('input[name="start_date"]').val('');
				$('input[name="end_date"]').val('');
				$('input[name="postal_code"]').val('');
				$('select[name="distance"]').val('');
				$('#btn-applyfilters').trigger('click');
			});
		</script>
	</div>
</script>

